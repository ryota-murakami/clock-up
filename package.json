{
  "name": "clock-up",
  "version": "0.1.0",
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "test": "node scripts/test.js --env=jsdom --verbose",
    "test:v": "node scripts/test.js --env=jsdom --verbose",
    "test:cov": "node scripts/test.js --env=jsdom --collectCoverage",
    "codecov": "codecov -t $CODECOV_TOKEN",
    "lint": "eslint src",
    "lint:fix": "eslint --fix src"
  },
  "jest": {
    "collectCoverage": false,
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}",
      "!src/index.js",
      "!src/registerServiceWorker.js",
      "!src/globalCSS.js",
      "!src/types/**/*"
    ],
    "setupFiles": [
      "<rootDir>/config/shims.js",
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node",
      "mjs"
    ]
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.4.0",
    "@storybook/addon-links": "^3.4.0",
    "@storybook/react": "^3.4.0",
    "autoprefixer": "^8.2.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-loader": "^7.1.4",
    "babel-plugin-styled-components": "^1.3.0",
    "babel-preset-react-app": "^3.1.1",
    "babel-runtime": "^6.26.0",
    "case-sensitive-paths-webpack-plugin": "^2.1.2",
    "chalk": "^2.3.2",
    "codecov": "^3.0.0",
    "css-loader": "^0.28.11",
    "dotenv": "^5.0.1",
    "enzyme": "^3.1.1",
    "enzyme-adapter-react-16": "^1.0.4",
    "enzyme-to-json": "^3.2.1",
    "eslint": "4.19.1",
    "eslint-config-react-app": "^2.0.1",
    "eslint-loader": "2.0.0",
    "eslint-plugin-flowtype": "2.46.1",
    "eslint-plugin-import": "2.10.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-prettier": "^2.3.1",
    "eslint-plugin-react": "^7.5.1",
    "extract-text-webpack-plugin": "3.0.2",
    "flow-bin": "^0.69.0",
    "jest-localstorage-mock": "^2.1.0",
    "prettier": "^1.7.4",
    "react-dev-utils": "^5.0.1",
    "webpack-dev-server": "^2.9.0"
  },
  "dependencies": {
    "apollo-cache-inmemory": "^1.1.12",
    "apollo-client": "^2.2.8",
    "apollo-link-http": "^1.0.0",
    "auth0-lock": "^11.5.2",
    "file-loader": "1.1.11",
    "fs-extra": "^5.0.0",
    "graphql": "^0.13.2",
    "graphql-tag": "^2.5.0",
    "history": "^4.7.2",
    "html-webpack-plugin": "^3.0.0",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.3.0",
    "postcss-loader": "2.1.3",
    "promise": "8.0.1",
    "raf": "3.4.0",
    "react": "^16.3.1",
    "react-apollo": "^2.1.3",
    "react-dom": "^16.3.1",
    "react-redux": "^5.0.7",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.9",
    "react-scripts": "^1.1.4",
    "react-spinners": "^0.3.1",
    "react-test-renderer": "^16.3.1",
    "style-loader": "0.20.3",
    "styled-components": "^3.2.5",
    "sw-precache-webpack-plugin": "0.11.5",
    "url-loader": "^1.0.1",
    "webpack": "^3.11.0",
    "webpack-manifest-plugin": "2.0.0-rc.2",
    "whatwg-fetch": "^2.0.4"
  }
}
